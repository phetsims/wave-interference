{
  "name": "wave-interference",
  "version": "2.1.0-dev.2",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/wave-interference.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "WAVE_INTERFERENCE",
    "preload": [
      "../sherpa/lib/imageprocessing-labs-fft-1.0.0.js"
    ],
    "phetLibs": [
      "twixt",
      "griddle",
      "bamboo"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simFeatures": {
      "supportsSound": true
    },
    "simulation": true,
    "supportsOutputJS": true
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "globals": {
      "FFT": "readonly"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts"
        ],
        "rules": {
          "@typescript-eslint/explicit-module-boundary-types": "off",
          "bad-typescript-text": "off",
          "explicit-method-return-type": "off",
          "no-simple-type-checking-assertions": "off"
        }
      }
    ]
  }
}