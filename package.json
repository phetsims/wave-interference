{
  "name": "wave-interference",
  "version": "2.0.18",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/wave-interference.git"
  },
  "devDependencies": {
    "grunt": "~1.0.0"
  },
  "phet": {
    "requirejsNamespace": "WAVE_INTERFERENCE",
    "preload": [
      "../sherpa/lib/imageprocessing-labs-fft-1.0.0.js"
    ],
    "phetLibs": [
      "twixt",
      "griddle"
    ],
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet"
    ],
    "simulation": true,
    "screenNameKeys": [
      "WAVE_INTERFERENCE/screen.waves",
      "WAVE_INTERFERENCE/screen.interference",
      "WAVE_INTERFERENCE/screen.slits",
      "WAVE_INTERFERENCE/screen.diffraction"
    ]
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_es6_eslintrc.js",
    "globals": {
      "FFT": false
    }
  }
}